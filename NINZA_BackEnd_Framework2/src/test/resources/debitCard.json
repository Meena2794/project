{	
	"request":{
	"method":"POST",
	"url":"/debitCard",
	"bodyPatterns":[
	{
	"matchesJsonPath":"$.[?(@.debitCardNum =='1234567887654321')]"
	},
	{
	"matchesJsonPath":"$.[?(@.cvv =='543')]"
	},
	{
